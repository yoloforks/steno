{
  "name": "@stenodb/writer",
  "version": "3.2.0",
  "description": "Specialized fast async file writer",
  "keywords": [
    "fs",
    "file",
    "write",
    "writer",
    "asynchronous",
    "fast",
    "race",
    "condition",
    "atomic",
    "writing",
    "safe"
  ],
  "homepage": "https://github.com/typicode/steno",
  "bugs": {
    "url": "https://github.com/typicode/steno/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typicode/steno.git"
  },
  "funding": "https://github.com/sponsors/typicode",
  "license": "MIT",
  "author": "Typicode <typicode@gmail.com>",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/steno.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/steno.cjs",
      "import": "./dist/steno.esm.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/steno.cjs",
    "dist/steno.esm.js",
    "dist/index.d.ts"
  ],
  "scripts": {
    "dev": "microbundle watch",
    "build": "del-cli dist && microbundle -i src/index.ts -i src/benchmark.ts --format esm,cjs --target node",
    "test": "npm run build && xv dist",
    "benchmark": "npm run build && node dist/benchmark.esm.js"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "xv": "1.1.1"
  },
  "engines": {
    "node": ">=14.16"
  }
}
